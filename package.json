  "homepage": "https://stonecrafter.github.io/",
  "dependencies": {
    "axios": "^0.21.1",
    "gh-pages": "^2.0.0",
    "lodash": "^4.17.19",
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.4.2",
    "react-anchor-link-smooth-scroll": "^1.0.11",
